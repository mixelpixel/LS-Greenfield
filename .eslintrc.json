{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "arrow-body-style": 0,
      "comma-dangle": 0,
      "consistent-return": 0,
      "import/no-unresolved": 0,
      "linebreak-style": 0,
      "max-len": 0,
      "no-param-reassign": 0,
      "no-plusplus": 0,
      "no-extend-native": 0,
      "no-restricted-properties": 0,
      "no-return-assign": 0,
      "no-unused-vars": 0,
      "no-useless-constructor": 0,
      "no-useless-return": 0
    }
}
